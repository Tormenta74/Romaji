kansu fibo:seisu <- seisu:n {
    to = n 0 {
        kisu 0
    }
    to = n 1 {
        kisu 1
    }

    seisu n2
    seisu n1

    n1 <- - n 1
    n1 <- fibo(n1)

    n2 <- - n 2
    n2 <- fibo(n2)

    kisu + n1 n2
}


omo:kyo {
    seisu index <- 0
    seisu result

    naka < index 30 {
        result <- fibo(index)
        tsutaeru("fibo(" index ") = " result)
        ++ index
    }

    shi
}
