

kansu fibo:seisu <-
    seisu:n
{
    seisu n2

    to < n 2 {
        kisu n
    } ta {
        seisu n1
    }

    n1 <- - n 1
    n2 <- - n 2

    kisu + fibo(n1) fibo(n2)
}


omo:kyo
{
    seisu index <- 0

    naka < index 10 {
        seisu result <- fibo(index)
        ++ index
        tsutaeru("fibo(" index ") = " result)
    }

    shi
}
