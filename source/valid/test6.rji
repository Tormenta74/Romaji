# a demonstration of the use of all of the operators
omo:kyo
{
    seisu a <- 12
    tsutaeru ( + a 2 ) # prints 14
    tsutaeru ( - a 2 ) # prints 10
    tsutaeru ( * a 0 ) # prints 0
    tsutaeru ( / a 2 ) # prints 6
    tsutaeru ( % a 3 ) # prints 0
    tsutaeru ( = a 2 ) # prints nise
    tsutaeru ( > a 2 ) # prints shin
    tsutaeru ( < a 2 ) # prints nise
    tsutaeru ( >= a 2 ) # prints shin
    tsutaeru ( <= a 2 ) # prints nise
    tsutaeru ( | shin nise ) # prints shin
    tsutaeru ( & shin nise ) # prints nise
    shi
}
