# a demonstration of the use of all of the operators
omo:kyo
{
    mojiretsu s[] <- "a string variable"
    seisu a <- 12
    a<- + a 2  tsutaeru (a) # prints 14
    a<- - a 2  tsutaeru (a) # prints 10
    a<- * a 0  tsutaeru (a) # prints 0
    a<- / a 2  tsutaeru (a) # prints 6
    a<- % a 3  tsutaeru (a) # prints 0
    tsutaeru (shin) # prints shin
    tsutaeru (nise) # prints nise
    tsutaeru(s)
    tsutaeru("a string literal")
    shi
}
