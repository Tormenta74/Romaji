kansu i:seisu <- seisu:a {
    a <- + a 1
    kisu a
}

kansu h:seisu <- seisu:a {
    a <- i(a)
    kisu a
}

kansu g:seisu <- seisu:a {
    a <- h(a)
    kisu a
}

kansu f:seisu <- seisu:a {
    a <- g(a)
    kisu a
}

omo:kyo {
    seisu a <- + 2 3
    tsutaeru(a)
    f(a)
    tsutaeru(a)
    shi
}
