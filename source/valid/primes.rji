# definimos una función que determine si un número es primo

kansu is_prime?:shinri <- seisu:n {
    seisu i <- 2

    to = % n 2 0 {
        kisu nise
    }

    naka <= * i i n {
        to = % n i 0 {
            kisu nise
        }
        ++ i
    }
    kisu shin
}

# punto de entrada

omo:kyo {
    seisu p <- 0

    naka shin {
        to is_prime? (p) {
            tsutaeru(p)
        }
        ++ p
    }
    shi
}
